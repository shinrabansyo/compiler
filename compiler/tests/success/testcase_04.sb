// if 文のテスト

fn main() {
    if (test_01() != 1) { return 1; }
    if (test_02() != 1) { return 2; }
    if (test_03() != 1) { return 3; }
    return 0;
}

fn test_01() {
    if (0 == 0) {
        return 1;
    } else {
        return 0;
    }
}

fn test_02() {
    if (10 + 20 == (10 - 20) + 40) {
        return 1;
    } else {
        return 0;
    }
}

fn test_03() {
    if (0 == 1) {
        return 0;
    } else if(0 == 2) {
        return 0;
    } else {
        return 1;
    }
}
